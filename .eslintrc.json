{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console":"off",
        "eol-last":"off",
        "semi":"off",
        "no-tabs":"off",
        "indent":"off",
        "import/no-extraneous-dependencies":"disable"
    }
}
